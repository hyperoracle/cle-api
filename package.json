{
  "name": "@hyperoracle/zkgraph-api",
  "version": "0.1.9",
  "description": "API for interacting with Hyper Oracle zkGraphs",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs",
    "prepublishOnly": "npm run build",
    "release": "npm publish --access public",
    "testexec": "node tests/testexec.js",
    "testprove": "node tests/testprove.js",
    "testsetup": "node tests/testsetup.js",
    "testverify": "node tests/testverify.js"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyperoracle/zkgraph-api.git"
  },
  "keywords": [
    "zkgraph",
    "api",
    "hyperoracle",
    "ho",
    "zkg"
  ],
  "author": "Norman (nom4dv3) & Suning (msfew)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hyperoracle/zkgraph-api/issues"
  },
  "homepage": "https://github.com/hyperoracle/zkgraph-api#readme",
  "dependencies": {
    "bn.js": "^5.2.1",
    "ethers": "^5.7.2",
    "js-yaml": "^4.1.0",
    "zkWasm-service-helper": "github:DelphinusLab/zkWasm-service-helper",
    "web3-eth-contract": "^1.8.2",
    "semver": "^7.5.4"
  },
  "files": [
    "api",
    "common",
    "requests",
    "index.js",
    "README.md",
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^20.4.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "rollup": "^3.29.3",
    "rollup-plugin-dts": "^6.0.2",
    "rollup-plugin-esbuild": "^6.0.1"
  }
}
